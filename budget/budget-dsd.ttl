@prefix obeu-r:         <http://openbudgets.eu/resource/dsd/> .
@prefix obeu:           <http://openbudgets.eu/vocabulary/dsd/> .
@prefix nuts:           <http://ec.europa.eu/eurostat/ramon/ontologies/geographic.rdf#> .
@prefix qb:             <http://purl.org/linked-data/cube#> .
@prefix rdfs:           <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sdmx-attribute: <http://purl.org/linked-data/sdmx/2009/attribute#> .
@prefix sdmx-dimension: <http://purl.org/linked-data/sdmx/2009/dimension#> .

obeu-r:Budget1 a qb:DataStructureDefinition ;
  rdfs:label "Sample budget Data Structure Definition"@en ;
  rdfs:comment "Made for D1.2 of OpenBudgets.eu"@en ;
  qb:component [
	rdfs:label "Budgetary unit"@en ;
	qb:dimension obeu:budgetaryUnit
  ], [
	rdfs:label "Fiscal period"@en ;
	qb:dimension obeu:refPeriod
  ], [
    qb:dimension obeu:programmeClassification
  ], [
	qb:attribute obeu:currency ;
  ], [
	qb:measure obeu:amount
  ].
  
 