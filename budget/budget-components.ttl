@prefix rdf:			<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:           <http://www.w3.org/2000/01/rdf-schema#> .
@prefix qb:             <http://purl.org/linked-data/cube#> .
@prefix xsd:			<http://www.w3.org/2001/XMLSchema#> .
@prefix sdmx-attribute: <http://purl.org/linked-data/sdmx/2009/attribute#> .
@prefix sdmx-dimension: <http://purl.org/linked-data/sdmx/2009/dimension#> .
@prefix sdmx-concept:	<http://purl.org/linked-data/sdmx/2009/concept#> .
@prefix sdmx-measure:    <http://purl.org/linked-data/sdmx/2009/measure#> .
@prefix dbpedia-owl:	<http://dbpedia.org/ontology/> .
@prefix interval:		<http://reference.data.gov.uk/def/intervals/> .
@prefix nuts:           <http://ec.europa.eu/eurostat/ramon/ontologies/geographic.rdf#> .

@prefix obeu-r:         <http://openbudgets.eu/resource/dsd/> .
@prefix obeu:           <http://openbudgets.eu/vocabulary/dsd/> .

obeu:budgetaryUnit a rdf:Property, qb:DimensionPropery ;
	rdfs:label "budgetary unit"@en .
	
obeu:refPeriod a rdf:Property, qb:DimensionProperty ;
    rdfs:label "reference period"@en ;
    rdfs:subPropertyOf sdmx-dimension:refPeriod ;
    rdfs:range interval:Interval ;
    qb:concept sdmx-concept:refPeriod .

obeu:currency a rdf:Property, qb:AttributeProperty ;
    rdfs:label "currency"@en ;
    rdfs:subPropertyOf sdmx-attribute:currency ;
    rdfs:range dbpedia-owl:Currency ;
    qb:concept sdmx-concept:currency .

obeu:programmeClassification a rdf:Property, qb:DimensionProperty ;
	rdfs:label "programme classification"@en .

obeu:amount a rdf:Property, qb:MeasureProperty ;
	rdfs:label "amount"@en ;
	rdfs:subPropertyOf sdmx-measure:obsValue ;
	rdfs:range xsd:decimal ;
	qb:concept sdmx-concept:obsValue .
  
 